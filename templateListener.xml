<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="https://www.woltlab.com" xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://www.woltlab.com https://www.woltlab.com/XSD/tornado/templateListener.xsd">
	<import>
		<templatelistener name="raffleFormTab">
			<environment>user</environment>
			<templatename>messageFormTabs</templatename>
			<eventname>tabMenuTabs</eventname>
			<templatecode><![CDATA[{include file='__messageFormRaffleTab'}]]></templatecode>
		</templatelistener>
		<templatelistener name="tabMenuContents">
			<environment>user</environment>
			<templatename>messageFormTabs</templatename>
			<eventname>tabMenuContents</eventname>
			<templatecode><![CDATA[{include file='__messageFormRaffle'}]]></templatecode>
		</templatelistener>
		<templatelistener name="raffleThreadPostList">
			<environment>user</environment>
			<templatename>threadPostList</templatename>
			<eventname>beforeMessageText</eventname>
			<templatecode><![CDATA[{include file='__raffleThreadPostList'}]]></templatecode>
		</templatelistener>
		<templatelistener name="raffleThreadPostListButtons">
			<environment>user</environment>
			<templatename>threadPostList</templatename>
			<eventname>messageFooterButtons</eventname>
			<templatecode><![CDATA[{include file='__raffleThreadPostListButtons'}]]></templatecode>
		</templatelistener>
		<templatelistener name="raffleThreadListIcons">
			<environment>user</environment>
			<templatename>threadList</templatename>
			<eventname>statusIcons</eventname>
			<templatecode><![CDATA[{include file='__raffleThreadListIcons'}]]></templatecode>
		</templatelistener>
		<templatelistener name="raffleCopyright">
			<environment>user</environment>
			<templatename>pageFooterCopyright</templatename>
			<eventname>copyright</eventname>
			<templatecode><![CDATA[{include file='__raffleCopyright'}]]></templatecode>
		</templatelistener>
	</import>
</data>
